    }
  ],
  "license": "MIT",
  "devDependencies": {
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "rollup": "^1.27.13",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/solid-user-search"
  },
  "dependencies": {
    "@comunica/actor-rdf-join-nestedloop": "^1.10.0",
    "@solid/react": "^1.8.0",
